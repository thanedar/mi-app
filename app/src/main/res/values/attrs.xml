        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="BorderImageView">
        <attr name="border_image_width" format="dimension" />
        <attr name="border_image_color" format="color" />
    </declare-styleable>

    <declare-styleable name="WidgetTypeFaceFolks">
        <attr name="type_face" format="string" />
    </declare-styleable>

    <declare-styleable name="BaseTheme">
        <attr name="actionBarIconColor" format="color" />
        <attr name="actionBarInsetStart" format="dimension" />
